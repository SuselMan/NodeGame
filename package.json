{
  "name": "nodegame",
  "version": "0.0.1",
  "description": "Multiplayer game",
  "homepage": "https://github.com/SuselMan/NodeGame",
  "main": "server.js",
  "scripts": {
    "start": "npm run serve",
    "dev": "npm-run-all --parallel server client physics stats",
    "server": "npm-run-all --parallel server:*",
    "server:webpack": "webpack --config webpack/webpack.server.js --watch",
    "server:nodemon": "nodemon dist/server/server.js",
    "stats": "webpack --config webpack/webpack.stats.js --watch",
    "client": "webpack --config webpack/webpack.client.js --watch",
    "physics": "webpack --config webpack/webpack.physics.js --watch",
    "build": "webpack --config webpack/webpack.client.prod.js && webpack --config webpack/webpack.physics.prod.js && webpack --config webpack/webpack.server.js && webpack --config webpack/webpack.stats.prod.js",
    "serve": "node dist/server/server.js",
    "prettier": "prettier --write 'src/**/*.ts'",
    "docker:start": "npm i --only=production && npm run serve",
    "docker:publish": "npm run build && docker-compose up -d --build",
    "docker:up": "docker-compose up -d --build",
    "lint": "tslint 'src/**/*.{ts,tsx}'"
  },
  "author": {
    "name": "Ilya Pavlukhin",
    "url": "https://github.com/suselman"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SuselMan/NodeGame"
  },
  "license": "MIT",
  "dependencies": {
    "@geckos.io/phaser-on-nodejs": "^1.0.3",
    "axios": "^0.19.0",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "helmet": "^3.21.1",
    "moment": "^2.24.0",
    "phaser": "^3.22.0",
    "pidusage": "^2.0.17",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "source-map-support": "^0.5.11",
    "uuid": "^3.3.2",
    "mongoose": "^5.7.12",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.1.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "multer": "^1.4.2",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.16",
    "@types/helmet": "0.0.43",
    "@types/jsdom": "^12.2.3",
    "@types/matter-js": "^0.10.9",
    "@types/node": "^11.11.3",
    "@types/pidusage": "^2.0.1",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.4",
    "@types/mongodb": "^3.3.12",
    "@types/mongoose": "^5.5.32",
    "@types/multer": "^1.3.10",
    "mongodb": "^3.3.5",
    "copy-webpack-plugin": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "object-sizeof": "^1.3.0",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2",
    "typescript-tslint-plugin": "^0.5.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
